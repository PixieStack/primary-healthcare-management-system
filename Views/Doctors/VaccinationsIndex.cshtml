                            <a asp-controller="Doctors" asp-action="VaccinationTravelMedicine" class="btn-buy">Travel Medicine</a>
                        </div>
                    </div>
                    
                    <div class="col-lg-3 col-md-6" data-aos="zoom-in" data-aos-delay="400">
                        <div class="box">
                            <h3 style="color: #6D3790;">Vaccine Administration</h3>
                            <img src="~/dashboards/images/vaccines/doctor/admin.png" class="img-fluid" alt="">

                            <a asp-controller="Doctors" asp-action="VaccinationAdminister" class="btn-buy">Vaccine Administration</a>
                        </div>
                    </div>
                    
                    <div class="col-lg-3 col-md-6 mx-auto" data-aos="zoom-in" data-aos-delay="400">
                        <div class="box">
                            <h3 style="color: blue;">Patient History Reports</h3>
                            <img src="~/dashboards/images/vaccines/doctor/Patient_report.png" class="img-fluid" alt="">
                            <a asp-controller="Doctors" asp-action="VaccinationHistory" class="btn-buy">Patient History Reports</a>
                        </div>
                    </div>
                    <div class="col-lg-3 col-md-6 mx-auto" data-aos="zoom-in" data-aos-delay="400">
